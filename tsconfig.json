{
  "compilerOptions": {
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "strict": true,
    "noEmit": true,
    "target": "esnext",
    "jsx": "react",
    "allowJs": true,
    "lib": ["dom", "es2015", "es2017"],
    "noImplicitReturns": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "baseUrl": "src",
    "paths": {
      "@components/*": ["components/*"],
      "@images/*": ["assets/images/*"],
      "@assets/*": ["assets/*"],
      "@pages/*": ["pages/*"],
      "@utils/*": ["utils/*"],
      "@hooks/*": ["utils/hooks/*"],
  }
  },
  "exclude": [
    "./node_modules",
    "./public",
    "./.cache",
    "./gatsby-config.ts",
    "./gatsby-node.ts",
    "./gatsby-browser.ts",
    "./gatsby-ssr.ts"
  ],
  "include": ["./src/**/*"]
}
